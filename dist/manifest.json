{
  "name": "DBDark",
  "version": "0.2.7",
  "manifest_version": 2,
  "description": "Dark theme for Databricks",
  "icons": {
    "48": "img/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.databricks.com/*",
        "https://*.azuredatabricks.net/*"
      ],
      "css": ["index.css"],
      "run_at": "document_idle"
    }
  ]
}
