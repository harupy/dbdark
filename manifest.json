{
  "name": "Databricks Notebook Enhancer",
  "version": "0.1",
  "manifest_version" : 2,
  "description": "Enhance the editor functionality of Databricks notebook",
  "icons": {
    "48": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.databricks.com/*"
      ],
      "js": ["contentscript.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["main.js"]
}